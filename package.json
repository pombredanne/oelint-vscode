{
  "name": "oelint-vscode",
  "displayName": "oelint-adv",
  "description": "Lint your bitbake files",
  "license": "MIT",
  "homepage": "https://github.com/priv-kweihmann/oelint-vscode",
  "version": "1.0.0",
  "publisher": "kweihmann",
  "repository": {
    "type": "git",
    "url": "https://github.com/priv-kweihmann/oelint-vscode.git"
  },
  "bugs": {
    "url": "https://github.com/priv-kweihmann/oelint-vscode/issues"
  },
  "engines": {
    "vscode": "^1.14.0"
  },
  "categories": [
    "Linters"
  ],
  "keywords": [
    "bitbake",
    "lint"
  ],
  "activationEvents": [
    "onLanguage:bitbake"
  ],
  "main": "./out/src/extension",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/node": "^12.0.8",
    "@types/vscode": "^1.14.0",
    "typescript": "^3.5.1",
    "vscode-test": "^1.3.0"
  },
  "dependencies": {
    "tsc": "^1.20150623.0"
  }
}
